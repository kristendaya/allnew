rsconf = {
    _id : "replica",
    members : [
        { _id : 0, host : "127.0.0.1:10000", priority : 2 },
        { _id : 1, host : "127.0.0.1:10001", priority : 1 },
        { _id : 2, host : "127.0.0.1:10002", priority : 1 },
        { _id : 3, host : "127.0.0.1:10003", priority : 1, arbiterOnly: true }
    ]
}

rs.initiate(rsconf)

rs.status()